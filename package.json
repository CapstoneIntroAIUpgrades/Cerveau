{
  "name": "cerveau",
  "version": "2.0.0",
  "description": "The Node.js implementation of a multi-game server intended for games with AIs within the Cadre framework.",
  "repository": {
    "type": "git",
    "url": "git://github.com/siggame/Cerveau"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@types/argparse": "1.0.34",
    "@types/body-parser": "1.17.0",
    "@types/delay": "2.0.1",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/express-handlebars": "0.0.30",
    "@types/fs-extra": "5.0.4",
    "@types/gaussian": "1.1.1",
    "@types/json5": "0.0.29",
    "@types/node": "10.5.6",
    "@types/sanitize-filename": "1.1.28",
    "@types/seedrandom": "2.4.27",
    "@types/uuid": "3.4.3",
    "@types/v8-profiler": "0.0.5",
    "argparse": "1.0.10",
    "body-parser": "1.18.3",
    "chess.js": "github:JacobFischer/chess.js",
    "delay": "3.0.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "fs-extra": "7.0.0",
    "gaussian": "1.1.0",
    "json5": "1.0.1",
    "lark-websocket": "git://github.com/JacobFischer/lark-websocket.git#95a4947ee1b20bc3ae8155f3c405d61fcc1f97d9",
    "moment": "2.22.2",
    "sanitize-filename": "1.6.1",
    "seedrandom": "2.4.3",
    "ts-node": "7.0.0",
    "ts-typed-events": "1.1.1",
    "tsconfig-paths": "3.5.0",
    "uuid": "3.3.2",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "husky": "0.14.3",
    "shx": "0.3.2",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.3.1",
    "tslint-no-math-random": "1.0.1",
    "typescript": "3.0.1"
  },
  "_optionalDependencies": {
    "v8-profiler": "5.7.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "ln": "shx cp ./node_modules/ ./dist/node_modules/",
    "watch": "tsc --watch",
    "clean": "shx rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "apidoc": "apidoc -i ./src/web/routes/ -o ./api-docs",
    "test": "npm run lint && npm run build",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "fix": "tslint --config tslint.json --project tsconfig.json --fix --force",
    "precommit": "npm test",
    "postmerge": "npm install",
    "postrewrite": "npm install"
  },
  "_moduleAliases": {
    "src": "dist/"
  }
}
